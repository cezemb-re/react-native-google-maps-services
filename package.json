{
  "name": "@cezemb-re/react-native-google-maps-services",
  "title": "React Native Google Maps Services",
  "version": "0.0.1",
  "description": "React Native library to use Google Maps Services on Android and IOS from their native official SDK's",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "README.md",
    "android",
    "ios",
    "react-native-google-maps-services.podspec",
    "dist"
  ],
  "scripts": {
    "lint": "eslint --ext .ts \"src/**/*.{ts, tsx}\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cezemb-re/react-native-google-maps-services.git",
    "baseUrl": "https://github.com/cezemb-re/react-native-google-maps-services"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "keywords": [
    "react-native",
    "google",
    "maps",
    "services",
    "places",
    "autocomplete",
    "android",
    "ios"
  ],
  "author": "Lucien Perouze <lucien.perouze@cezembre.co>",
  "license": "MIT",
  "licenseFilename": "LICENSE",
  "peerDependencies": {
    "react": "^16.8.1",
    "react-native": ">=0.60.0-rc.0 <1.0.x"
  },
  "devDependencies": {
    "@types/react": "^16.9.35",
    "@types/react-native": "^0.62.11",
    "@typescript-eslint/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^3.0.1",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "eslint-plugin-react-native": "^3.8.1",
    "prettier": "^2.0.5",
    "react": "^16.9.0",
    "react-native": "^0.61.5",
    "semantic-release": "^17.2.3",
    "typescript": "^3.9.3"
  },
  "dependencies": {},
  "bugs": {
    "url": "https://github.com/cezemb-re/react-native-google-maps-services/issues"
  },
  "homepage": "https://github.com/cezemb-re/react-native-google-maps-services#readme"
}
